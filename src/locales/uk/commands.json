{
  "info": {
    "name": "Інформація",
    "emoji": ":mag_right:",
    "aliases": ["inf", "information"],
    "commands": {
      "info": {
        "description": "Інформація про мене",
        "info": "Базова інформація про бота Ayi, як: розробник, версія, профілів та діалогів у базі, ",
        "usage": "info"
      },
      "help": {
        "description": "Список усіх команд",
        "usage": "help (category/command)",
        "example": "help Модерація"
      },
      "profile": {
        "description": "Переглянути профіль користувача",
        "usage": "profile (user)",
        "example": "profile @rastgame"
      },
      "card": {
        "description": "Створити карточку привітання",
        "usage": "card"
      }
    }
  },
  "utility": {
    "name": "Утиліти",
    "emoji": ":hammer_and_wrench:",
    "aliases": ["utility", "utils"],
    "commands": {
      "user": {
        "description": "Інформація про користувача",
        "usage": "user (user)",
        "example": "user @rastgame"
      },
      "invite": {
        "description": "Додати бота до групи",
        "usage": "invite [dialog.id]",
        "example": "invite 123"
      }, 
      "leave": {
        "description": "Видалити бота з групи",
        "info": "Бот автоматично покине групу і дані про групу та її користувачів, будуть автоматично видалені з базі даних",
        "usage": "leave"
      },
      "encrypt": {
        "description": "Зашифрувати текст",
        "usage": "encrypt [key] [text]",
        "example": "encrypt ключ секретний текст"
      },
      "decrypt": {
        "description": "Розшифрувати текст",
        "usage": "decrypt [key] [encrypted]",
        "example": ["decrypt ключ abc123:def456", "decrypt ключ acc5f5c266a805458e260f54a4554efd:e04fab95d4b9585a4b5a3132e1eafec9d71026b20843c43e130b375dace545b8"]
      }
    }
  },
  "moderation": {
    "name": "Модерація",
    "emoji": ":shield:",
    "aliases": ["moderation", "mod"],
    "commands": {
      "kick": {
        "description": "Видалити користувача з групи",
        "usage": "kick [user]",
        "example": ["kick @rastgame", "kick 123"],
        "permissions": [16, 999],
        "info": "Видаляє користувача з групи назавжди"
      },
      "kickunactive": {
        "description": "Видалити неактивних користувачів",
        "usage": "kickunactive [час]",
        "example": ["kickunactive 7d", "kickunactive 1m", "kickunactive 30d"],
        "info": "Формат часу: h (години), d (дні), m (місяці), y (роки)"
      },
      "mute": {
        "description": "Замутити користувача",
        "usage": "mute [user] [час] (причина)",
        "example": "mute @rastgame 60 спам"
      },
      "clear": {
        "description": "Очистити повідомлення",
        "usage": "clear [кількість]",
        "example": ["clear 10", "clear 25"],
        "info": "Максимум 40 повідомлень за раз"
      }
    }
  },
  "admin": {
    "name": "Адміністрація",
    "emoji": ":gear:",
    "aliases": ["admin", "administration"],
    "commands": {
      "settings": {
        "description": "Налаштування групи",
        "usage": "settings [show|set] [параметр] [значення]",
        "example": ["settings show", "settings set levels true", "settings set welcome {user} Привіт!", "settings set goodbye {user} Бувай!"],
        "info": "Параметри: `moderation, levels, economy, welcome, goodbye`\nᯓ Для *welc/bye*: true/false або текст\nДодатково ➜ {a.prefix}help placeholders\n"
      },
      "token": {
        "description": "Встановлення токену",
        "usage": "token [dialog] [токен]",
        "example": "token 292 y.h8N2cV4pL0r9G7kS5tY1wQ6mXz3BfZq",
        "info": "Ви маєте бути власником групи\nЦю команду вписуємо тільки в особистих повідомленнях :double-exclamation:"
      },
      "perms": {
        "description": "Керування правами користувачів",
        "usage": "perms [show|give|take] (user) (permissions...)",
        "example": ["perms show @rastgame", "perms give @rastgame MODERATOR", "perms take @rastgame ALL"],
        "permissions": [128],
        "info": "Можна вказувати кілька прав одразу або ALL для всіх\nПрава: WRITE, DELETE_MSG, EDIT_MSG, EDIT_GROUP, KICK, BAN, MUTE, MANAGE_PERMS, MANAGE_ADMINS, ADMIN\nMODERATOR = DELETE_MSG + KICK + MUTE"
      }
    }
  },
  "ai": {
    "name": "ШІ",
    "emoji": ":dizzy:",
    "aliases": ["ai", "artificial"],
    "commands": {
      "ask": {
        "description": "Поставити запитання штучному інтелекту",
        "usage": "ask [промт]",
        "example": "ask Як справи?"
      }
    }
  },
  "levels": {
    "name": "Рівні",
    "emoji": ":zap:",
    "aliases": ["levels", "level"],
    "commands": {
      "rank": {
        "description": "Картка рівня користувача",
        "usage": "rank (user)",
        "example": "rank @rastgame"
      }
    }
  },
  "unix": {
    "name": "Додатково",
    "emoji": ":sparkles:",
    "aliases": ["unique", "special"],
    "commands": {
      "placeholders": {
        "description": "Плейсхолдері (привітання/прощавання)",
        "header": "Змінні, що автоматично підставляють дані у повідомлення",
        "usage": "placeholders",
        "info": "Всі доступні на цю міть:\n{user} - згадка користувача\n{user.name} - ім'я користувача\n{user.link} - посилання на користувача\n{dialog.name} - назва групи\n{dialog.members} - кількість учасників\n{date} - поточна дата\n{time} - поточний час"
      }
    }
  }
}