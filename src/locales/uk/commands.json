{
  "Інформація": {
    "emoji": ":mag_right:",
    "aliases": ["info", "information"],
    "commands": {
      "info": {
        "description": "Інформація про мене",
        "usage": "info",
        "example": "info"
      },
      "help": {
        "description": "Список усіх команд",
        "usage": "help (category/command)",
        "example": "help Модерація"
      },
      "profile": {
        "description": "Переглянути профіль користувача",
        "usage": "profile (user)",
        "example": "profile @rastgame"
      },
      "card": {
        "description": "Створити карточку привітання",
        "usage": "card",
        "example": "card"
      }
    }
  },
  "Утиліти": {
    "emoji": ":hammer_and_wrench:",
    "aliases": ["utility", "utils"],
    "commands": {
      "user": {
        "description": "Інформація про користувача",
        "usage": "user (user)",
        "example": "user @rastgame"
      },
      "invite": {
        "description": "Додати бота до групи",
        "usage": "invite [dialog.id]",
        "example": "invite 123"
      }, 
      "leave": {
        "description": "Видалити бота з групи",
        "usage": "leave",
        "example": "leave"
      },
      "encrypt": {
        "description": "Зашифрувати текст",
        "usage": "encrypt [key] [text]",
        "example": "encrypt ключ секретний текст"
      },
      "decrypt": {
        "description": "Розшифрувати текст",
        "usage": "decrypt [key] [encrypted]",
        "example": ["decrypt ключ abc123:def456", "decrypt ключ acc5f5c266a805458e260f54a4554efd:e04fab95d4b9585a4b5a3132e1eafec9d71026b20843c43e130b375dace545b8"]
      }
    }
  },
  "Модерація": {
    "emoji": ":shield:",
    "aliases": ["moderation", "mod"],
    "commands": {
      "kick": {
        "description": "Видалити користувача з групи",
        "usage": "kick [user]",
        "example": ["kick @rastgame", "kick 123"],
        "info": "Тільки власник групи може використовувати цю команду"
      },
      "kickunactive": {
        "description": "Видалити неактивних користувачів",
        "usage": "kickunactive [час]",
        "example": ["kickunactive 7d", "kickunactive 1m", "kickunactive 30d"],
        "info": "Формат часу: h (години), d (дні), m (місяці), y (роки)"
      },
      "mute": {
        "description": "Замутити користувача",
        "usage": "mute [user] [час] (причина)",
        "example": "mute @rastgame 60 спам"
      },
      "clear": {
        "description": "Очистити повідомлення",
        "usage": "clear [кількість]",
        "example": ["clear 10", "clear 25"],
        "info": "Максимум 40 повідомлень за раз"
      }
    }
  },
  "Адміністрація": {
    "emoji": ":gear:",
    "aliases": ["admin", "administration"],
    "commands": {
      "settings": {
        "description": "Налаштування групи",
        "usage": "settings [show|set|token]",
        "example": "settings show"
      },
      "welcome": {
        "description": "Налаштування привітання",
        "usage": "welcome (текст)",
        "example": "welcome Привіт {user}!"
      },
      "goodbye": {
        "description": "Налаштування прощавання",
        "usage": "goodbye (текст)",
        "example": "goodbye До побачення {user}!"
      }
    }
  },
  "ШІ": {
    "emoji": ":dizzy:",
    "aliases": ["ai", "artificial"],
    "commands": {
      "ask": {
        "description": "Поставити запитання штучному інтелекту",
        "usage": "ask [промт]",
        "example": "ask Як справи?"
      }
    }
  },
  "Рівні": {
    "emoji": ":zap:",
    "aliases": ["levels", "level"],
    "commands": {
      "rank": {
        "description": "Картка рівня користувача",
        "usage": "rank (user)",
        "example": "rank @rastgame"
      }
    }
  }
}